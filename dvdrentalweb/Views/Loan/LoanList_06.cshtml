@model IEnumerable<dvdrentalweb.Models.Loan>
@{
    ViewData["Title"] = "LoanList_06";
}

<div class="dropdown">
  <button class="btn btn-primary dropdown-toggle" type="button" data-toggle="dropdown">Dropdown Example
  <span class="caret"></span></button>
    <ul class="dropdown-menu">
        @foreach (var item in Model)
        {
           <li><a href="#">@Html.DisplayFor(modelItem => item.MemberFirstName) @Html.DisplayFor(modelItem => item.MemberLastName)</a></li>
        }
    </ul>
</div>


