@model IEnumerable<dvdrentalweb.Models.Member>
@{
    ViewData["Title"] = "MemberList_08";
}

<h1>Activities of Members</h1>

<table class="table">

    <thead>
        <tr>
            <th>
               Member's Name
            </th>
            <th>
               Total DVD Out
            </th>
            <th>
               Total DVD Allowed
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model) {
                <tr>
                    <td>
                        @item.MemberFirstName @item.MemberLastNamae
                    </td>
                    <td>
                        @item.TotalLoans
                    </td>
                    <td>
                        @item.TotalAllowedLoans
                    </td>
                </tr>
        }
    </tbody>
</table>

